{
    "builds": [
      {
        "src": "qquiz_app/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "maxLambdaSize": "100mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "qquiz_app/wsgi.py"
      }
    ]
  }
  